
@model ScoreMe.UI.Models.ApplicationInformationVM
@{
    ViewBag.Title = "Məlumat bazasındakı tətbiqlərin siyahısı";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-12">


        <div class="clearfix"></div><br />

        <div class="well bs-component">

            @*<a class="btn btn-warning pull-right btn-icon" href="@Url.Action("Create","ApplicationInformation")"><span class="fa fa-plus"></span> Əlavə et</a>*@

            <div class="clearfix"></div>
            <div class="table-responsive">
                <table class="table table-bordered" style="margin:0;">
                    <tbody>
                        <tr>
                       
                            <th class="col-md-4" style="font-size:14px; text-align:center; vertical-align:middle; background-color: gainsboro;">
                                @Html.DisplayNameFor(model => model.AppName)
                                <input onkeyup="Search(this, 'ApplicationInformation', 'AppNameIndex', 'Name')" class="form-control" placeholder="axtar" />
                            </th>
                         

                            <th class="col-md-2" style="font-size:14px; text-align:center; vertical-align:middle; background-color:gainsboro;">
                             Əməliyyat
                            </th>
                        </tr>
                    </tbody>
                </table>
                <div id="AjaxPaginationList">
                    @Html.Partial("AppNamePartialIndex")
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/pagedList.js"></script>




